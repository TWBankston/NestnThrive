{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "color": {
            "custom": true,
            "customDuotone": false,
            "customGradient": true,
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "palette": [
                {
                    "color": "#202329",
                    "name": "Charcoal",
                    "slug": "charcoal"
                },
                {
                    "color": "#161415",
                    "name": "Near Black",
                    "slug": "near-black"
                },
                {
                    "color": "#E0A456",
                    "name": "Warm Gold",
                    "slug": "warm-gold"
                },
                {
                    "color": "#E6B87A",
                    "name": "Light Gold",
                    "slug": "light-gold"
                },
                {
                    "color": "#B08047",
                    "name": "Bronze",
                    "slug": "bronze"
                },
                {
                    "color": "#6E5032",
                    "name": "Warm Brown",
                    "slug": "warm-brown"
                },
                {
                    "color": "#F7F4EF",
                    "name": "Off White",
                    "slug": "off-white"
                },
                {
                    "color": "#FFFFFF",
                    "name": "White",
                    "slug": "white"
                }
            ]
        },
        "layout": {
            "contentSize": "800px",
            "wideSize": "1200px"
        },
        "spacing": {
            "padding": true,
            "margin": true,
            "blockGap": true,
            "units": ["px", "em", "rem", "%", "vh", "vw"]
        },
        "typography": {
            "customFontSize": true,
            "dropCap": false,
            "fluid": true,
            "fontFamilies": [
                {
                    "fontFamily": "Georgia, 'Times New Roman', serif",
                    "name": "Heading",
                    "slug": "heading"
                },
                {
                    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, sans-serif",
                    "name": "Body",
                    "slug": "body"
                }
            ],
            "fontSizes": [
                {
                    "fluid": {
                        "min": "0.875rem",
                        "max": "1rem"
                    },
                    "name": "Small",
                    "size": "1rem",
                    "slug": "small"
                },
                {
                    "fluid": {
                        "min": "1rem",
                        "max": "1.125rem"
                    },
                    "name": "Medium",
                    "size": "1.125rem",
                    "slug": "medium"
                },
                {
                    "fluid": {
                        "min": "1.125rem",
                        "max": "1.5rem"
                    },
                    "name": "Large",
                    "size": "1.5rem",
                    "slug": "large"
                },
                {
                    "fluid": {
                        "min": "1.5rem",
                        "max": "2.25rem"
                    },
                    "name": "X-Large",
                    "size": "2.25rem",
                    "slug": "x-large"
                },
                {
                    "fluid": {
                        "min": "2rem",
                        "max": "3rem"
                    },
                    "name": "XX-Large",
                    "size": "3rem",
                    "slug": "xx-large"
                }
            ],
            "lineHeight": true
        },
        "useRootPaddingAwareAlignments": true
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--off-white)",
            "text": "var(--wp--preset--color--charcoal)"
        },
        "elements": {
            "button": {
                "border": {
                    "radius": "4px"
                },
                "color": {
                    "background": "var(--wp--preset--color--warm-gold)",
                    "text": "var(--wp--preset--color--near-black)"
                },
                ":hover": {
                    "color": {
                        "background": "var(--wp--preset--color--light-gold)"
                    }
                },
                ":focus": {
                    "color": {
                        "background": "var(--wp--preset--color--light-gold)"
                    }
                }
            },
            "h1": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading)",
                    "fontWeight": "600"
                },
                "color": {
                    "text": "var(--wp--preset--color--near-black)"
                }
            },
            "h2": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading)",
                    "fontWeight": "600"
                },
                "color": {
                    "text": "var(--wp--preset--color--near-black)"
                }
            },
            "h3": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading)",
                    "fontWeight": "600"
                },
                "color": {
                    "text": "var(--wp--preset--color--near-black)"
                }
            },
            "h4": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading)",
                    "fontWeight": "600"
                },
                "color": {
                    "text": "var(--wp--preset--color--near-black)"
                }
            },
            "h5": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading)",
                    "fontWeight": "600"
                }
            },
            "h6": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading)",
                    "fontWeight": "600"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--warm-gold)"
                },
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--bronze)"
                    }
                }
            }
        },
        "spacing": {
            "blockGap": "1.5rem",
            "padding": {
                "top": "0",
                "right": "var(--wp--preset--spacing--30)",
                "bottom": "0",
                "left": "var(--wp--preset--spacing--30)"
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--body)",
            "fontSize": "var(--wp--preset--font-size--medium)",
            "lineHeight": "1.6"
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header",
            "title": "Header"
        },
        {
            "area": "footer",
            "name": "footer",
            "title": "Footer"
        }
    ]
}

